      <td><%= @salary.basic_wage %></td>
      <td><%= @salary.post_wage %></td>
      <td><%= @salary.subsidy %></td>
      <td><%= @salary.bonus %></td>
      <td><%= @salary.performance_fee %></td>
      <td><%= @salary.salary %></td>
      <td><%= @salary.endowment_insurance %></td>
      <td><%= @salary.medical_insurance %></td>
      <td><%= @salary.jobless_insurance %></td>
      <td><%= @salary.housing_fund %></td>
      <td><%= @salary.tax %></td>
      <td><%= @salary.actual_salary %></td>
    </tr>
  </tbody>
</table>
