              <li class="list-group-item">
                公共通道:
                <% if house.public_passage.present? %>
                <%= t(house.public_passage,:scope=>"house.level.narrow") %>
                <% end %>
              </li>
              <li class="list-group-item">套内硬装:
                <% if house.house_upgrade.present? %>
                <%= t(house.house_upgrade,:scope=>"house.flag.you_wu") %>
