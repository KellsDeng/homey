<h1>index</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>id</th>
      <th>address</th>
      <th>created_at</th>

    </tr>
  </thead>
  <tbody>
    <% @houses.each do |house|%>
      <tr>
        <td><%= link_to(house.house_id,admin_house_path(house))  %></td>
        <td><%= house.address %></td>
        <td><%= house.created_at %></td>
        
      </tr>
    <%end%>
  </tbody>
</table>
