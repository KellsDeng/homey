<h1>house index</h1>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>id</th>
      <th>address</th>
      <th>created_at</th>
      <th>options</th>
    </tr>
  </thead>
  <tbody>
    <% @houses.each do |house|%>
      <tr>
        <td><%= link_to(house.id,admin_house_path(house))  %></td>
        <td><%= house.address %></td>
        <td><%= house.created_at %></td>
        <td>
          <%= link_to('edit',edit_admin_house_path(house),class:"btn btn-info") %>
          <%= link_to('destroy',admin_house_path(house),method: :delete,data:{confirm:"are you sure?"},class:"btn btn-danger") %>
        </td>
      </tr>
    <%end%>
  </tbody>
</table>
